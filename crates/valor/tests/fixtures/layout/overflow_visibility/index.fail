<!doctype html>
<meta charset="utf-8" />
<title>overflow visibility</title>
<body>
  <!-- : enable when overflow is fully wired and geometry matches -->
  <!-- Computed style should report overflow: visible/hidden -->
  <div id="visible" style="width: 50px; height: 20px; overflow: visible; background: #ddd;">
    <div style="width: 100px; height: 10px;"></div>
  </div>
  <div id="hidden" style="width: 50px; height: 20px; overflow: hidden; background: #ccc;">
    <div style="width: 100px; height: 10px;"></div>
  </div>
</body>

